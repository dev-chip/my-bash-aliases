# utils
alias clr='clear'

# git
alias g='git'
alias ga='git add'
alias gb='git branch'
alias gc='git commit -m'
alias gcb='git checkout -b'
alias gco='git checkout'
alias gl='git log --oneline --graph'
alias gr='git revert'
alias gs='git status'

# navigation
alias ..='cd ..'
alias home='cd ~'

# display filestructure
alias ll='ls -la'
alias tree='tree -Csuh'

# start programs
alias qtdesigner='/usr/lib/x86_64-linux-gnu/qt5/bin/designer'

# search grep history
alias gh='history|grep'

# basically, a verbose cp
alias cpv='rsync -ah --info=progress2'

# verbose
alias mv='mv -v'
alias cp='cp -v'
alias ln='ln -v'

